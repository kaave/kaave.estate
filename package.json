{
  "name": "kaave-estate",
  "version": "1.0.0",
  "private": true,
  "description": "kaave-estate",
  "engines": {
    "node": "18.14.2",
    "yarn": "not_allowed_yarn",
    "npm": "not_allowed_npm"
  },
  "scripts": {
    "dev": "run-s gen dev:*",
    "dev:gatsby": "gatsby develop --host 0.0.0.0",
    "dev:css-types": "pnpm gen:css-types -- --watch",
    "catalog": "ladle serve",
    "gen": "run-p gen:*",
    "gen:css-types": "tcm -e -p '**/*.module.css' src",
    "gen:routes": "ts-node scripts/createLink/index.ts",
    "build": "run-s gen build:*",
    "build:gatsby": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "fix": "run-p fix:*",
    "fix:scripts": "eslint --fix 'src/**/*.{ts,tsx}' && prettier --write 'src/**/*.{ts,tsx}'",
    "fix:styles": "stylelint --fix 'src/**/*.css' && prettier --write 'src/**/*.css'",
    "lint": "run-p lint:*",
    "lint:scripts": "eslint 'src/**/*.{ts,tsx}'",
    "lint:styles": "stylelint 'src/**/*.css'",
    "typecheck": "tsc --noEmit -p ./tsconfig.json"
  },
  "dependencies": {
    "@mdx-js/react": "^2.2.1",
    "@reach/router": "^1.3.4",
    "gatsby": "^5.3.3",
    "gatsby-plugin-google-gtag": "^5.4.0",
    "gatsby-plugin-image": "^3.3.2",
    "gatsby-plugin-layout": "^4.3.0",
    "gatsby-plugin-manifest": "^5.4.0",
    "gatsby-plugin-mdx": "^5.3.1",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-postcss": "^6.3.0",
    "gatsby-plugin-robots-txt": "^1.8.0",
    "gatsby-plugin-sharp": "^5.3.2",
    "gatsby-plugin-sitemap": "^6.3.1",
    "gatsby-plugin-tsconfig-paths": "^1.0.5",
    "gatsby-remark-prismjs": "^7.3.0",
    "gatsby-remark-prismjs-title": "^1.0.0",
    "gatsby-source-filesystem": "^5.3.1",
    "gatsby-transformer-remark": "^6.3.2",
    "gatsby-transformer-sharp": "^5.3.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5"
  },
  "devDependencies": {
    "@ladle/react": "^2.4.5",
    "@swc/core": "^1.3.24",
    "@swc/helpers": "^0.4.14",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^8.0.0",
    "@types/node": "^18.11.17",
    "@types/reach__router": "^1.3.11",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "autoprefixer": "^10.4.13",
    "bezier-easing": "^2.1.0",
    "change-case": "^4.1.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-regexp": "^1.11.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "eslint-plugin-unicorn": "^45.0.2",
    "eslint-plugin-vitest": "^0.0.21",
    "fs-extra": "^11.1.0",
    "gatsby-plugin-portal": "^1.0.7",
    "glob": "^8.0.3",
    "markdown-loader": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-calc": "^8.2.4",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^15.1.0",
    "postcss-nesting": "^10.2.0",
    "postcss-url": "^10.1.3",
    "prettier": "^2.8.1",
    "prettier-plugin-organize-imports": "^3.2.1",
    "regenerator-runtime": "^0.13.11",
    "sort-package-json": "^2.1.0",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recess-order": "^3.1.0",
    "stylelint-config-recommended": "^9.0.0",
    "ts-node": "^10.9.1",
    "typed-css-modules": "^0.7.2",
    "typescript": "^4.9.4"
  }
}
